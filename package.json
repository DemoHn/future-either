{
  "name": "future-either",
  "version": "0.1.3",
  "description": "An extension type of 'Future<L, R>' to support tri-state object",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
